          Serial.print("\t");
          j++;
          unsigned long fin = micros();
          float durasi = fin - init;
          delay(time_need - 25 - (durasi * 0.001));
        }
        unsigned long time_count = micros();
        Serial.println(time_count, 10);
        //Serial.print("\t");
      }
    }
  }
